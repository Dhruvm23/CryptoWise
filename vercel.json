{
    "functions": {
        "api/*.js": {
            "runtime": "@vercel/node@3.0.4"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/"
        }
    ]
}